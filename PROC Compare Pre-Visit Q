libname compare "/folders/myfolders";
run;

proc import out=compare.master datafile="/folders/myfolders/Master Age 4 previsit.csv" 
dbms=csv;
getnames=yes;
run;

proc import out=compare.mom_previsit datafile="/folders/myfolders/Age 4_momprevisit.xlsx" 
dbms=xlsx;
getnames=yes;
run;

/**Rename Datasets**/
data master_q; set compare.master; run; 
data mother_q; set compare.mom_previsit; run;

%macro a(x,y,z);
proc compare BASE=mother_q COMPARE=master_q;
TITLE &x;
ID study_id;
VAR &y;
WITH &z;
run;
%mend;

